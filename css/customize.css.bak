.cp article th[scope="col"] { text-align: center; vertical-align: middle; }
/*---ul , ol*/
.cp article ul, .cp article ol { margin: .5em 1em; padding: 0; }
.cp article ul li, .cp article ol li { margin: 0 0 .5em 0; }
